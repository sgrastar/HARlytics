{
    "manifest_version": 3,
    "name": "HAR Analyzer",
    "version": "0.0.1",
    "description": "A tool for analyzing HAR files",
    "icons": {
      "16": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    },
    "action": {
      "default_icon": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs"
    ]
}